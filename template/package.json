{
  "name": "react-native-boilerplate",
  "version": "1.1.0",
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "yarn run eslint --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "format": "yarn run lint -- --fix",
    "pretty": "prettier --write --config .prettierrc.js ./src/**/*.{ts,tsx}",
    "clear": "watchman watch-del-all && rm -rf /tmp/metro-bundler-cache-* && rm -rf /tmp/haste-map-react-native-packager-* && react-native start --reset-cache",
    "storybook": "start-storybook -p 7007",
    "rename": "react-native-rename",
    "pods": "cd ios && pod install && cd ..",
    "nodeify": "rn-nodeify --hack --install",
    "i18next:extract": "i18next 'src/**/**/*.tsx' --config 'src/I18n/i18n-parser.config.js'",
    "release": "standard-version",
    "release:major": "standard-version -- --release-as major",
    "release:minor": "standard-version -- --release-as minor",
    "release:patch": "standard-version -- --release-as patch",
    "release:version": "standard-version -- --release-as",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@apollo/client": "3.2.5",
    "@react-native-community/async-storage": "1.12.1",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/native": "5.7.3",
    "@react-navigation/stack": "5.9.0",
    "@tradle/react-native-http": "^2.0.0",
    "apollo3-cache-persist": "0.8.0",
    "browserify-zlib": "~0.1.4",
    "buffer": "^4.9.1",
    "dns.js": "^1.0.1",
    "events": "^1.0.0",
    "graphql": "15.4.0",
    "https-browserify": "~0.0.0",
    "i18next": "17.3.1",
    "isomorphic-fetch": "2.2.1",
    "path-browserify": "0.0.0",
    "punycode": "^1.2.4",
    "query-string": "6.13.1",
    "react": "18.1.0",
    "react-i18next": "10.13.2",
    "react-native": "0.70.2",
    "react-native-bootsplash": "2.2.5",
    "react-native-code-push": "6.3.0",
    "react-native-crypto": "^2.1.0",
    "react-native-gesture-handler": "^2.6.0",
    "react-native-level-fs": "^3.0.0",
    "react-native-localize": "^2.0.1",
    "react-native-os": "^1.0.1",
    "react-native-randombytes": "^3.0.0",
    "react-native-reanimated": "1.13.0",
    "react-native-safe-area-context": "^4.3.3",
    "react-native-screens": "2.10.1",
    "readable-stream": "1.0.33",
    "stream-browserify": "^1.0.0",
    "string_decoder": "~0.10.25",
    "styled-components": "^5.3.5",
    "timers-browserify": "^1.0.1",
    "url": "~0.10.1",
    "util": "~0.10.3",
    "vm-browserify": "0.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@commitlint/cli": "9.1.2",
    "@commitlint/config-conventional": "9.1.2",
    "@react-native-community/eslint-config": "^2.0.0",
    "@storybook/addon-actions": "5.3.21",
    "@storybook/addon-links": "5.3.21",
    "@storybook/addons": "5.3.21",
    "@storybook/react-native": "5.3.21",
    "@storybook/react-native-server": "5.3.21",
    "@types/enzyme": "3.10.5",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/jest": "^26.0.23",
    "@types/jsdom": "12.2.4",
    "@types/node": "12.12.54",
    "@types/react": "16.9.55",
    "@types/react-native": "^0.69.5",
    "@types/react-native-dotenv": "0.2.0",
    "@types/react-test-renderer": "^18.0.0",
    "@types/styled-components": "^5.1.26",
    "@types/styled-components-react-native": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "awesome-typescript-loader": "5.2.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "8.1.0",
    "babel-plugin-module-resolver": "3.2.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.4",
    "enzyme-to-json": "3.5.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "4.0.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.20.6",
    "husky": "^8.0.1",
    "i18next-parser": "^3.6.0",
    "jest": "^26.6.3",
    "jest-styled-components": "6.3.4",
    "jsdom": "15.2.1",
    "lint-staged": "^11.0.1",
    "metro-react-native-babel-preset": "0.72.3",
    "prettier": "^2.3.2",
    "react-docgen-typescript-loader": "3.7.2",
    "react-docgen-typescript-webpack-plugin": "1.1.0",
    "react-dom": "17.0.1",
    "react-native-dotenv": "0.2.0",
    "react-native-rename": "2.4.1",
    "react-native-screens": "^3.17.0",
    "react-test-renderer": "18.1.0",
    "redux-mock-store": "1.5.4",
    "rn-nodeify": "^10.3.0",
    "standard-version": "^9.3.1",
    "typescript": "^4.4.4"
  },
  "jest": {
    "preset": "react-native"
  },
  "react-native": {
    "zlib": "browserify-zlib",
    "console": "console-browserify",
    "constants": "constants-browserify",
    "crypto": "react-native-crypto",
    "dns": "dns.js",
    "domain": "domain-browser",
    "http": "@tradle/react-native-http",
    "https": "https-browserify",
    "os": "react-native-os",
    "path": "path-browserify",
    "querystring": "querystring-es3",
    "fs": "react-native-level-fs",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "stream": "stream-browserify",
    "timers": "timers-browserify",
    "tty": "tty-browserify",
    "vm": "vm-browserify",
    "tls": false
  },
  "browser": {
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "_stream_readable": "readable-stream/readable",
    "_stream_transform": "readable-stream/transform",
    "_stream_writable": "readable-stream/writable",
    "console": "console-browserify",
    "constants": "constants-browserify",
    "crypto": "react-native-crypto",
    "dns": "dns.js",
    "domain": "domain-browser",
    "fs": "react-native-level-fs",
    "http": "@tradle/react-native-http",
    "https": "https-browserify",
    "os": "react-native-os",
    "path": "path-browserify",
    "querystring": "querystring-es3",
    "stream": "stream-browserify",
    "timers": "timers-browserify",
    "tls": false,
    "tty": "tty-browserify",
    "vm": "vm-browserify",
    "zlib": "browserify-zlib"
  },
  "packageManager": "yarn@3.2.3"
}
