{
  "name": "react-native-boilerplate",
  "version": "1.1.0",
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "yarn run eslint --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "format": "yarn run lint -- --fix",
    "pretty": "prettier --write --config .prettierrc.js ./src/**/*.{ts,tsx}",
    "clear": "watchman watch-del-all && rm -rf /tmp/metro-bundler-cache-* && rm -rf /tmp/haste-map-react-native-packager-* && react-native start --reset-cache",
    "storybook": "start-storybook -p 7007",
    "rename": "react-native-rename",
    "pods": "cd ios && pod install && cd ..",
    "nodeify": "rn-nodeify --hack --install",
    "i18next:extract": "i18next 'src/**/**/*.tsx' --config 'src/I18n/i18n-parser.config.js'",
    "release": "standard-version",
    "release:major": "standard-version -- --release-as major",
    "release:minor": "standard-version -- --release-as minor",
    "release:patch": "standard-version -- --release-as patch",
    "release:version": "standard-version -- --release-as",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@apollo/client": "3.7.2",
    "@react-native-community/async-storage": "1.12.1",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/native": "6.0.16",
    "@react-navigation/stack": "6.3.7",
    "@tradle/react-native-http": "^2.0.1",
    "apollo3-cache-persist": "0.14.1",
    "browserify-zlib": "~0.2.0",
    "buffer": "^6.0.3",
    "dns.js": "^1.0.1",
    "events": "^3.3.0",
    "graphql": "16.6.0",
    "https-browserify": "~1.0.0",
    "i18next": "22.0.8",
    "intl-pluralrules": "^1.3.1",
    "isomorphic-fetch": "3.0.0",
    "path-browserify": "1.0.1",
    "punycode": "^2.1.1",
    "query-string": "7.1.3",
    "react": "18.2.0",
    "react-i18next": "12.1.0",
    "react-native": "0.71.1",
    "react-native-bootsplash": "4.4.0",
    "react-native-code-push": "7.0.5",
    "react-native-crypto": "^2.2.0",
    "react-native-gesture-handler": "^2.8.0",
    "react-native-level-fs": "^3.0.1",
    "react-native-localize": "^2.2.4",
    "react-native-os": "^1.2.6",
    "react-native-randombytes": "^3.6.1",
    "react-native-reanimated": "2.13.0",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "3.18.2",
    "readable-stream": "4.2.0",
    "stream-browserify": "^3.0.0",
    "string_decoder": "~1.3.0",
    "styled-components": "^5.3.6",
    "timers-browserify": "^2.0.12",
    "url": "~0.11.0",
    "util": "~0.12.5",
    "vm-browserify": "1.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/runtime": "^7.20.6",
    "@commitlint/cli": "17.3.0",
    "@commitlint/config-conventional": "17.3.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@storybook/addon-actions": "6.5.14",
    "@storybook/addon-links": "6.5.14",
    "@storybook/addons": "6.5.14",
    "@storybook/react-native": "5.3.27",
    "@storybook/react-native-server": "5.3.23",
    "@types/enzyme": "3.10.12",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/jest": "^29.2.4",
    "@types/jsdom": "20.0.1",
    "@types/node": "18.11.11",
    "@types/react": "18.0.26",
    "@types/react-native": "^0.70.8",
    "@types/react-native-dotenv": "0.2.0",
    "@types/react-test-renderer": "^18.0.0",
    "@types/styled-components": "^5.1.26",
    "@types/styled-components-react-native": "^5.2.0",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "awesome-typescript-loader": "5.2.1",
    "babel-jest": "^29.3.1",
    "babel-loader": "9.1.0",
    "babel-plugin-module-resolver": "4.1.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.7",
    "enzyme-to-json": "3.6.2",
    "eslint": "^8.29.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.11",
    "husky": "^8.0.2",
    "i18next-parser": "^7.0.3",
    "jest": "^29.3.1",
    "jest-styled-components": "7.1.1",
    "jsdom": "20.0.3",
    "lint-staged": "^13.1.0",
    "metro-react-native-babel-preset": "0.73.7",
    "prettier": "^2.8.0",
    "react-docgen-typescript-loader": "3.7.2",
    "react-docgen-typescript-webpack-plugin": "1.1.0",
    "react-dom": "18.2.0",
    "react-native-dotenv": "3.4.2",
    "react-native-rename": "2.9.0",
    "react-native-screens": "^3.18.2",
    "react-test-renderer": "18.2.0",
    "redux-mock-store": "1.5.4",
    "rn-nodeify": "^10.3.0",
    "standard-version": "^9.5.0",
    "typescript": "^4.9.3"
  },
  "jest": {
    "preset": "react-native"
  },
  "react-native": {
    "zlib": "browserify-zlib",
    "console": "console-browserify",
    "constants": "constants-browserify",
    "crypto": "react-native-crypto",
    "dns": "dns.js",
    "domain": "domain-browser",
    "http": "@tradle/react-native-http",
    "https": "https-browserify",
    "os": "react-native-os",
    "path": "path-browserify",
    "querystring": "querystring-es3",
    "fs": "react-native-level-fs",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "stream": "stream-browserify",
    "timers": "timers-browserify",
    "tty": "tty-browserify",
    "vm": "vm-browserify",
    "tls": false
  },
  "browser": {
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "_stream_readable": "readable-stream/readable",
    "_stream_transform": "readable-stream/transform",
    "_stream_writable": "readable-stream/writable",
    "console": "console-browserify",
    "constants": "constants-browserify",
    "crypto": "react-native-crypto",
    "dns": "dns.js",
    "domain": "domain-browser",
    "fs": "react-native-level-fs",
    "http": "@tradle/react-native-http",
    "https": "https-browserify",
    "os": "react-native-os",
    "path": "path-browserify",
    "querystring": "querystring-es3",
    "stream": "stream-browserify",
    "timers": "timers-browserify",
    "tls": false,
    "tty": "tty-browserify",
    "vm": "vm-browserify",
    "zlib": "browserify-zlib"
  },
  "packageManager": "yarn@3.2.3"
}
